<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx"
       xmlns:fx="http://javafx.com/fxml" fx:controller="org.savingprivatenitti.controllers.widget.ProfileController">
    <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="140.0" spacing="10.0">
        <children>
            <ImageView fitHeight="20.0" fitWidth="20.0">
                <image>
                    <Image url="@../../icons/profile.png" />
                </image>
            </ImageView>
            <VBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="100.0">
                <children>
                    <Text fx:id="usernameText" strokeType="OUTSIDE" strokeWidth="0.0" text="Username" styleClass="h3" />
                    <Text fx:id="emailText" strokeType="OUTSIDE" strokeWidth="0.0" text="Email" styleClass="h4"/>
                </children>
            </VBox>
        </children>
        <fx:include source="Logout.fxml" />
        <FlowPane.margin>
            <Insets right="60.0" top="200.0" />
        </FlowPane.margin>
    </HBox>
    <FlowPane.margin>
        <Insets left="10.0" right="40.0" top="260.0" />
    </FlowPane.margin>
</VBox>